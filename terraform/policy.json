                "ecs:CreateService",
                "ecs:RunTask",
                "ecs:RegisterTaskDefinition",
                "ecs:SubmitContainerStateChange",
                "ecs:StopTask",
                "ecs:TagResource",
                "ecs:PutClusterCapacityProviders",
                "ecs:DeleteCapacityProvider",
                "ecs:List*",
                "ecs:UntagResource",
                "ecs:CreateTaskSet"
            ],
            "Resource": "*",
            "Condition": {
                "StringEquals": {
